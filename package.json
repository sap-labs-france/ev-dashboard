{
  "name": "ev-dashboard",
  "description": "Dashboard for Electric Vehicle charging station",
  "homepage": "https://github.com/LucasBrazi06/ev-dashboard",
  "version": "2.3.7",
  "repository": {
    "type": "git",
    "url": "https://github.com/LucasBrazi06/ev-dashboard.git"
  },
  "author": "SAP E-Mobility Labs France <e-mobility@sap.com>",
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "importSort": {
    ".js, .jsx, .es6, .es, .mjs": {
      "parser": "babylon",
      "style": "module",
      "options": {}
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module",
      "options": {}
    }
  },
  "scripts": {
    "git:sinit": "git submodule update --init --recursive --force",
    "git:sdiff": "git diff && git submodule foreach 'git diff'",
    "git:supdate": "git submodule update --remote --merge",
    "git:spush": "git push --recurse-submodules=on-demand",
    "git:sreset": "git submodule foreach 'git reset --hard'",
    "ngcc": "ngcc",
    "ng": "ng",
    "start": "npm run ngcc && ng serve --configuration=development",
    "start:ssl": "npm run ngcc && ng serve --configuration=development --ssl true --ssl-key ./ssl/localhost.key --ssl-cert ./ssl/localhost.cert",
    "start:prod": "npm run ngcc && cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" ng serve --prod",
    "start:prod:ssl": "npm run ngcc && cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" ng serve --prod --ssl true --ssl-key ./ssl/localhost.key --ssl-cert ./ssl/localhost.cert",
    "build": "npm run build:dev",
    "build:prod": "npm run ngcc && cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" ng build --prod --stats-json",
    "build:dev": "npm run ngcc && ng build --configuration=development --stats-json",
    "codeceptjstest": "npm run e2e",
    "tslint": "ng lint",
    "tslint:fix": "ng lint --fix=true",
    "e2e": "codeceptjs run --steps --reporter mochawesome",
    "clean-install": "rimraf package-lock.json && rimraf node_modules && npm cache clear --force && npm install",
    "npm-check": "npm-check",
    "ng:update": "ng update",
    "angular-update": "ng update @angular/cli @angular/core",
    "material-update": "ng update @angular/material",
    "angular11-update": "ng update @angular/core@11 @angular/cli@11",
    "material11-update": "ng update @angular/material@11",
    "webpack-analyze": "webpack-bundle-analyzer dist/stats.json",
    "import-sort": "npx import-sort-cli --write '{src,test}/**/*.ts{,x}'"
  },
  "dependencies": {
    "@agm/core": "^1.1.0",
    "@angular/animations": "~11.0.2",
    "@angular/cdk": "^11.0.0",
    "@angular/common": "~11.0.2",
    "@angular/compiler": "~11.0.2",
    "@angular/core": "~11.0.2",
    "@angular/forms": "~11.0.2",
    "@angular/material": "^11.0.0",
    "@angular/material-moment-adapter": "^11.0.0",
    "@angular/platform-browser": "~11.0.2",
    "@angular/platform-browser-dynamic": "~11.0.2",
    "@angular/router": "~11.0.2",
    "@auth0/angular-jwt": "^5.0.2",
    "@mat-datetimepicker/core": "^5.1.0",
    "@mat-datetimepicker/moment": "^5.1.0",
    "@ng-bootstrap/ng-bootstrap": "^8.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "angular-moment-picker": "^0.10.2",
    "angular2-chartjs": "^0.5.1",
    "bootstrap": "^4.5.3",
    "bootstrap-material-design": "^4.1.3",
    "bootstrap-notify": "^3.1.3",
    "bootstrap-select": "^1.13.18",
    "bootstrap-tagsinput": "^0.7.1",
    "chart.js": "^2.9.4",
    "chartjs-plugin-datalabels": "^0.7.0",
    "chartjs-plugin-zoom": "^0.7.7",
    "file-saver": "^2.0.5",
    "hammerjs": "^2.0.8",
    "http-status-codes": "^2.1.4",
    "jasny-bootstrap": "^4.0.0",
    "jquery": "^3.5.1",
    "moment": "^2.29.1",
    "moment-duration-format": "^2.3.2",
    "moment-timezone": "^0.5.32",
    "ngx-captcha": "^8.0.1",
    "ngx-daterangepicker-material": "^4.0.1",
    "ngx-google-places-autocomplete": "^2.0.5",
    "ngx-moment": "^5.0.0",
    "popper.js": "^1.16.1",
    "rxjs": "~6.6.0",
    "socket.io-client": "^3.0.3",
    "sweetalert2": "^10.10.1",
    "tslib": "^2.0.3",
    "zone.js": "~0.10.2"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.2",
    "utf-8-validate": "^5.0.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.2",
    "@angular/cli": "~11.0.2",
    "@angular/compiler-cli": "~11.0.2",
    "@types/bootstrap": "^5.0.0",
    "@types/chart.js": "^2.9.28",
    "@types/chartist": "^0.11.0",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "^3.6.2",
    "@types/jquery": "^3.5.4",
    "@types/mocha": "^8.0.4",
    "@types/moment-duration-format": "^2.2.2",
    "@types/node": "^12.19.6",
    "codeceptjs": "^3.0.2",
    "codelyzer": "^6.0.1",
    "cross-env": "^7.0.2",
    "faker": "^5.1.0",
    "husky": "^4.3.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jasmine": "~3.6.3",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "maildev": "^1.1.0",
    "mocha": "^8.2.1",
    "mochawesome": "^6.2.1",
    "moment-locales-webpack-plugin": "^1.2.0",
    "moment-timezone-data-webpack-plugin": "^1.3.0",
    "npm-check": "^5.9.2",
    "puppeteer": "^5.5.0",
    "rimraf": "^3.0.2",
    "rosie": "^2.0.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.3",
    "typescript": "~4.0.5",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^4.1.0"
  }
}
